{
  "name": "@pushowl/shogun-frontend-sdk",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/pushowl/pushowl-shogun",
  "bugs": {
    "url": "https://github.com/pushowl/pushowl-shogun/issues"
  },
  "author": "PushOwl",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "jest": "jest --no-cache",
    "jest:watch": "jest --watch",
    "test": "yarn jest",
    "copy-files": "copyfiles -u 1 ./public/** ./dist/",
    "build:tsc": "tsc -p ./tsconfig.build.json",
    "build": "yarn build:tsc && yarn copy-files",
    "start:tsc": "yarn build:tsc -w",
    "start": "yarn start:tsc",
    "typedoc": "typedoc --tsconfig ./tsconfig.build.json",
    "prepublishOnly": "yarn test && yarn build",
    "version": "yarn typedoc && git add docs"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "copyfiles": "^2.4.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.21.0",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0",
    "frontend-checkout": "^3.3.7-alpha.1",
    "frontend-customer": "^2.0.4",
    "react": "^17.0.0"
  },
  "dependencies": {
    "faker": "^5.2.0",
    "frontend-checkout": "^3.3.7-alpha.1",
    "frontend-customer": "^2.0.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "tslib": "^2.1.0"
  }
}
